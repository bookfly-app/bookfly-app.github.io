{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "wallflower",
	"version": "0.1.0",
	"identifier": "land.wallflower",
	"build": {
		"frontendDist": "../src/assets",
		"devUrl": "http://localhost:1420",
		"beforeDevCommand": "bun run dev",
		"beforeBuildCommand": "bun run build"
	},
	"app": {
		"windows": [
			{
				"title": "Wallflower",
				"resizable": true,
				"fullscreen": false,
				"maximized": true
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		]
	}
}
